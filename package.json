{
  "name": "feedback-doodles",
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "dev": "pnpm -r dev",
    "build": "pnpm -r build",
    "lint": "pnpm -r lint",
    "typecheck": "pnpm -r typecheck",
    "db:generate": "dotenv -e env/server.env -- drizzle-kit generate",
    "db:migrate": "dotenv -e env/server.env -- drizzle-kit migrate"
  },
  "devDependencies": {
    "@types/node": "20.0.0",
    "dotenv-cli": "^11.0.0"
  },
  "dependencies": {
    "drizzle-kit": "^0.31.8"
  }
}
